{
  "name": "MoodEntry",
  "type": "object",
  "properties": {
    "mood_level": {
      "type": "number",
      "minimum": 1,
      "maximum": 10,
      "description": "Mood rating from 1-10"
    },
    "mood_emoji": {
      "type": "string",
      "description": "Selected mood emoji"
    },
    "reflection": {
      "type": "string",
      "description": "Optional user reflection"
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "Mood tags like stress, anxiety, hope, etc."
    },
    "energy_level": {
      "type": "number",
      "minimum": 1,
      "maximum": 5,
      "description": "Energy level rating"
    }
  },
  "required": [
    "mood_level",
    "mood_emoji"
  ]
}